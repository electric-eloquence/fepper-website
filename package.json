    "pattern",
    "prototype",
    "prototyper",
    "prototyping",
    "template"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "chai": "4.x",
    "eslint": "5.x",
    "fepper": "^0.32.2",
    "fepper-utils": "^1.2.2",
    "fs-extra": "^7.0.1",
    "gulp": "https://github.com/electric-eloquence/gulp/tarball/v3-lts@3.9.7",
    "husky": "1.x",
    "mocha": "6.x"
