      {{> components-content-docpage-install }}
    {{/ docpage_install? }}
    {{# docpage_configure? }}
      {{> components-content-docpage-configure }}
    {{/ docpage_configure? }}
    {{# docpage_configure_1? }}
      {{> components-content-docpage-configure-1 }}
    {{/ docpage_configure_1? }}
    {{# docpage_run? }}
      {{> components-content-docpage-run }}
    {{/ docpage_run? }}
    {{# docpage_edit? }}
      {{> components-content-docpage-edit }}
    {{/ docpage_edit? }}
    {{# docpage_update? }}
      {{> components-content-docpage-update }}
    {{/ docpage_update? }}
